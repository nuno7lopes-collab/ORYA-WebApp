{
  "name": "orya",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "dev:all": "node scripts/dev-all.js",
    "reset": "scripts/reset-dev-servers.sh",
    "reset:hard": "scripts/reset-dev-servers.sh --aggressive --full --no-exclude",
    "delete": "prisma db execute --file scripts/db/cleanup-app_v3.sql",
    "build": "ORIGINAL_NODE_ENV=$NODE_ENV next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc -p tsconfig.typecheck.json --noEmit",
    "test": "vitest run",
    "worker": "node scripts/operations-loop.js",
    "cron:local": "node scripts/cron-loop.js",
    "chat:ws": "node scripts/chat-ws-server.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "db:env": "node -r ./scripts/load-env.js ./scripts/check-db-env.js",
    "db:preflight": "node ./scripts/db/preflight.js",
    "db:status": "npm run db:env && npm run db:preflight && node ./scripts/db/prisma-retry.js migrate status --schema=prisma/schema.prisma",
    "db:deploy": "npm run db:env && npm run db:preflight && node ./scripts/db/prisma-retry.js migrate deploy --schema=prisma/schema.prisma",
    "db:generate": "npm run db:env && npm run db:preflight && node ./scripts/db/prisma-retry.js generate --schema=prisma/schema.prisma",
    "db:gates": "node ./scripts/db/gates.js",
    "db:gates:offline": "DB_GATES_MODE=offline npm run db:gates",
    "gate:api-contract": "node scripts/v9_api_contract_gate.mjs",
    "gate:internal-secret": "node scripts/v9_internal_secret_gate.mjs",
    "gate:p0-errors": "node scripts/v9_p0_error_gate.mjs",
    "gate:parity": "node scripts/v9_parity_gate.mjs",
    "gate:runbooks": "node scripts/v9_runbook_gate.mjs",
    "gate:todo": "node scripts/v9_todo_gate.mjs",
    "v9:api-mapping": "node scripts/v9_api_frontend_mapping.mjs",
    "v9:inventory": "node scripts/v9_inventory.mjs",
    "covers:build": "node scripts/generate_cover_library.js",
    "covers:fetch": "node scripts/fetch_openverse_covers.js",
    "covers:thumbs": "node -r ts-node/register scripts/covers/generate-thumbs.ts",
    "covers:manifest": "node -r ts-node/register scripts/covers/sync-manifest.ts",
    "covers:validate": "node -r ts-node/register scripts/covers/validate-manifest.ts",
    "covers:all": "npm run covers:thumbs && npm run covers:manifest && npm run covers:validate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.2.0",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/pdfkit": "^0.17.4",
    "@types/seedrandom": "^3.0.8",
    "@upstash/redis": "^1.34.4",
    "@vercel/analytics": "^1.6.1",
    "framer-motion": "^12.23.24",
    "jose": "^5.9.3",
    "libphonenumber-js": "^1.12.31",
    "next": "^16.1.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-datepicker": "^8.9.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.6.0",
    "resend": "^4.8.0",
    "seedrandom": "^3.0.5",
    "stripe": "^19.3.1",
    "swr": "^2.3.6",
    "ws": "^8.19.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.9.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^41.0.2",
    "vitest": "^1.6.1"
  }
}
